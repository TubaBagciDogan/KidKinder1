
@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">
@Html.Action("PartialHead", "Default")

<body>
    <!-- Navbar Start -->
    @Html.Action("PartialNavbar", "Default")
    <!-- Navbar End -->
    <h3>Burası Contact Sayfası</h3>
    @Html.Action("ContactHeaderPartial", "Contact")

    <div class="container-fluid pt-5">
        <div class="container">
            <div class="text-center pb-2">
                <p class="section-title px-5">
                    <span class="px-2">İletişim</span>
                </p>
                <h1 class="mb-4">Sorularınız İçin Bize Ulaşın</h1>
            </div>
            <div class="row">
                <div class="col-lg-7 mb-5">
                    <div class="contact-form">
                        <div id="success"></div>
                        @Html.Action("ContactMessage", "Contact")
                    </div>
                </div>

                @Html.Action("ContactAddressPartial", "Contact")
            </div>
        </div>
    </div>
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3178.7254184868966!2d33.2134347!3d37.1829959!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14d98cffdc20bb2b%3A0xae36de50e0b77e62!2sAktekke%20Cami!5e0!3m2!1str!2str!4v1711367999594!5m2!1str!2str" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>

    <!-- Footer Start -->
    @Html.Action("PartialFooter", "Default")
    <!-- Footer End -->
    <!-- Back to Top -->
    @Html.Action("PartialScript", "Default")
</body>
</html>


